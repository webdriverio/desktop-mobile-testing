{
  "name": "@wdio/native-types",
  "version": "2.0.0",
  "description": "Type definitions for WebdriverIO Native Desktop Services (Electron and Tauri)",
  "homepage": "https://github.com/webdriverio/desktop-mobile-testing/tree/main/packages/native-types",
  "license": "MIT",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": [
      {
        "import": {
          "types": "./dist/esm/index.d.ts",
          "default": "./dist/esm/index.js"
        },
        "require": {
          "types": "./dist/cjs/index.d.ts",
          "default": "./dist/cjs/index.js"
        }
      },
      "./dist/cjs/index.js"
    ]
  },
  "engines": {
    "node": ">=18 || >=20"
  },
  "scripts": {
    "clean": "shx rm -rf dist coverage .turbo",
    "build": "tsx ../../scripts/build-package.ts",
    "dev": "tsx ../../scripts/build-package.ts --watch",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "@vitest/spy": "^4.0.16"
  },
  "devDependencies": {
    "@electron-forge/shared-types": "^7.10.2",
    "@electron/packager": "^19.0.1",
    "@types/node": "^25.0.3",
    "@wdio/globals": "catalog:default",
    "@wdio/types": "catalog:default",
    "builder-util": "^26.0.11",
    "electron": "catalog:default",
    "read-package-up": "^12.0.0",
    "shx": "^0.4.0",
    "typescript": "^5.9.3",
    "webdriverio": "catalog:default"
  },
  "files": ["dist", "LICENSE"],
  "repository": {
    "type": "git",
    "url": "https://github.com/webdriverio/desktop-mobile-testing.git",
    "directory": "packages/native-types"
  }
}
